<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.back.blog.category.mapper.CategoryMapper">
    <select id="getAllCategoryList" resultType="com.back.blog.category.domain.CategoryDomain">
        SELECT
            category_num,
            category_name,
            category_state
        FROM
            category
    </select>

</mapper>